{
    "version": 2,
    "builds": [
        {
            "src": "/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}